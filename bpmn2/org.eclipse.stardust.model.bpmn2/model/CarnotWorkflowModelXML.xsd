            <xsd:documentation>
                  The condition used to verify transition, default value is
                  "TRUE". Now deprecated for conditions beside TRUE and FALSE,
                  more complex conditions should be put into an expression element.
               </xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="false" name="forkOnTraversal" type="xsd:string"/>
        <xsd:attribute name="from" type="xsd:string">
          <xsd:annotation>
            <xsd:documentation>The unique model id of the starting activity.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="to" type="xsd:string">
          <xsd:annotation>
            <xsd:documentation>The unique model id of the ending activity.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="TransitionType" type="TransitionType"/>
  <xsd:complexType name="TriggersConnectionType">
    <xsd:complexContent>
      <xsd:extension base="IConnectionSymbol">
        <xsd:attribute name="startEventSymbol" type="xsd:string">
          <xsd:annotation>
            <xsd:documentation>The model oid of the start symbol.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="participantSymbol" type="xsd:string">
          <xsd:annotation>
            <xsd:documentation>
                  The model oid of the participant symbol.
               </xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="TriggersConnectionType" type="TriggersConnectionType"/>
  <xsd:complexType name="TriggerType">
    <xsd:complexContent>
      <xsd:extension base="IIdentifiableModelElement">
        <xsd:sequence>
          <xsd:element maxOccurs="unbounded" minOccurs="0" name="accessPoint" type="AccessPointType">
            <xsd:annotation>
              <xsd:documentation>The list of access points.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="unbounded" minOccurs="0" name="parameterMapping" type="ParameterMappingType">
            <xsd:annotation>
              <xsd:documentation>The list of parameter mappings.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="type" type="xsd:string">
          <xsd:annotation>
            <xsd:documentation>
                  The model id of one of the previously defined triggerType elements.
               </xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="TriggerType" type="TriggerType"/>
  <xsd:complexType name="TriggerTypeType">
    <xsd:complexContent>
      <xsd:extension base="IMetaType">
        <xsd:attribute name="panelClass" type="xsd:string"/>
        <xsd:attribute name="pullTrigger" type="xsd:string">
          <xsd:annotation>
            <xsd:documentation>
                  A boolean that indicates whether the trigger type describes a
                  pulltrigger. A pull trigger (e.g. mail) has its own trigger daemon.
               </xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="pullTriggerEvaluator" type="xsd:string"/>
        <xsd:attribute name="rule" type="xsd:string"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="TriggerTypeType" type="TriggerTypeType"/>
  <xsd:complexType name="UnbindActionType">
    <xsd:complexContent>
      <xsd:extension base="AbstractEventAction"/>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="UnbindActionType" type="UnbindActionType"/>
  <xsd:complexType name="ViewableType">
    <xsd:attribute name="viewable" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation>
                  The model oid of the viewable. Valid viewable model elements are
                  "activity", "application", "data",
                  "participant", "workflow process".
               </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:element name="ViewableType" type="ViewableType"/>
  <xsd:complexType name="ViewType">
    <xsd:complexContent>
      <xsd:extension base="IModelElement">
        <xsd:sequence>
          <xsd:element maxOccurs="unbounded" minOccurs="0" name="attribute" type="AttributeType">
            <xsd:annotation>
              <xsd:documentation>The list of extension attributes.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="0" name="description" type="DescriptionType">
            <xsd:annotation>
              <xsd:documentation>
                     An optional description of the view.
                  </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="unbounded" minOccurs="0" name="view" type="ViewType">
            <xsd:annotation>
              <xsd:documentation>
                     The list of further dependent views.
                  </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="unbounded" minOccurs="0" name="viewable" type="ViewableType">
            <xsd:annotation>
              <xsd:documentation>The list of viewables.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="name" type="xsd:string" use="required">
          <xsd:annotation>
            <xsd:documentation>The human friendly name of the view.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="ViewType" type="ViewType"/>
  <xsd:complexType name="WorksForConnectionType">
    <xsd:complexContent>
      <xsd:extension base="IConnectionSymbol">
        <xsd:attribute name="organizationSymbol" type="xsd:string">
          <xsd:annotation>
            <xsd:documentation>
                  The model oid of the organization symbol.
               </xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="participantSymbol" type="xsd:string">
          <xsd:annotation>
            <xsd:documentation>
                  The model oid of the participant symbol.
               </xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="WorksForConnectionType" type="WorksForConnectionType"/>
  <xsd:complexType name="XmlTextNode">
    <xsd:sequence>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="mixed" nillable="true" type="xsd:string"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="XmlTextNode" type="XmlTextNode"/>
</xsd:schema>
