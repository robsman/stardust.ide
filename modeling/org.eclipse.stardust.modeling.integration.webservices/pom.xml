<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>

  <parent>
    <groupId>org.eclipse.stardust.modeling.core</groupId>
    <artifactId>modeling-core-parent</artifactId>
    <version>9.9.9-SNAPSHOT</version>
    <relativePath>../pom.xml</relativePath>
  </parent>

  <artifactId>org.eclipse.stardust.modeling.integration.webservices</artifactId>

  <packaging>eclipse-plugin</packaging>    <dependencies>     <dependency>        <groupId>com.infinity.bpm</groupId>        <artifactId>stardust-engine-ws-cxf</artifactId>        <version>${ipp.version}</version>        <exclusions>           <exclusion>              <groupId>com.infinity.bpm</groupId>              <artifactId>carnot-engine</artifactId>           </exclusion>        </exclusions>     </dependency>     <dependency>        <groupId>commons-logging</groupId>        <artifactId>commons-logging</artifactId>        <version>1.1.1</version>     </dependency>  </dependencies>    <build>    <plugins>      <plugin>        <groupId>org.apache.maven.plugins</groupId>        <artifactId>maven-dependency-plugin</artifactId>        <executions>          <execution>            <id>embed-jars-no-version</id>            <configuration>              <includeArtifactIds>stardust-engine-ws-cxf</includeArtifactIds>            </configuration>          </execution>          <execution>            <id>embed-jars</id>            <configuration>              <includeArtifactIds>commons-logging</includeArtifactIds>            </configuration>          </execution>        </executions>      </plugin>    </plugins>  </build>

</project>
